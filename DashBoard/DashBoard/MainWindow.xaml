					<Setter Property="Fill" TargetName="rectangle1" Value="#FF8B8888"/>
					<Setter Property="Fill" TargetName="rectangle2" Value="#FF8B8888"/>
				</Trigger>
			</ControlTemplate.Triggers>
		</ControlTemplate>
